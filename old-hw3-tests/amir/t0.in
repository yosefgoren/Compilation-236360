
int addOne(bool flag, int num){
    int y = num + 1 - 1;
    if(y >= num + 1){
        printi(y);
        print("y is very cool\n");
    }
    else{
        while(y != 8){
            byte t = 8b;
            t = (byte)(t + 1);
            if(t > 1) break;
            continue;
        }
    }
    return 8b + num;
}

void main()
{
    int y = addOne(not true, addOne(false, 5 + 7));
    printi(y + 8b);
}

